{
  "name": "nuxt-vite-plugin-webfont-dl",
  "type": "module",
  "version": "0.1.1",
  "description": "A nuxt module to automatically install and configure the vite-plugin-webfont-dl vite plugin.",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/nuxt-vite-plugin-webfont-dl.git"
  },
  "keywords": [
    "css",
    "font",
    "font-optimization",
    "google-fonts",
    "nuxt",
    "nuxt-module",
    "self-hosted-fonts",
    "vite",
    "vite-plugin",
    "webfont",
    "webfonts"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "files": [
    "./dist"
  ],
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "bumplog": "changelogen --bump --hideAuthorEmail",
    "depcheck": "depcheck",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint --max-warnings 0 && stylelint './**/*.{css,scss,vue}' -i ./.gitignore --mw 0",
    "lint:fix": "eslint --fix --max-warnings 0 && stylelint './**/*.{css,scss,vue}' -i ./.gitignore --fix --mw 0",
    "prepack": "pnpm run build",
    "release": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build && changelogen --hideAuthorEmail --push --release && npm publish",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "typecheck": "pnpm run dev:prepare && nuxt typecheck && cd ./playground && nuxt typecheck",
    "unused-exports": "ts-unused-exports ./tsconfig.json"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2",
    "vite-plugin-webfont-dl": "^3.11.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.6.1",
    "@kikiutils/eslint-config": "^5.0.0",
    "@kikiutils/eslint-plugin-tailwindcss": "^3.19.0",
    "@kikiutils/eslint-plugin-vue": "^10.2.2",
    "@kikiutils/stylelint-config": "^0.3.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.21.0",
    "@types/node": "^25.0.1",
    "@vitest/coverage-v8": "^4.0.15",
    "changelogen": "^0.6.2",
    "consola": "^3.4.2",
    "depcheck": "^1.4.7",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "nuxt": "^4.2.2",
    "ts-unused-exports": "^11.0.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ],
    "overrides": {
      "tailwindcss": "^3.4.18"
    }
  }
}
